{
  "name": "meal-planner",
  "description": "Meal Planner",
  "private": true,
  "type": "module",
  "version": "0.5.1",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "bump": "changeset version",
    "clean:localstorage": "rm vite-storage*",
    "dev": "vite",
    "emulate": "firebase emulators:start --import seed-data",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "release": "run-p build && firebase deploy",
    "save-seed": "firebase emulators:export ./seed-data --force",
    "test": "sh -c 'vitest run; code=$?; run-p clean:localstorage; exit \"$code\"'",
    "test:cov": "sh -c 'vitest run --coverage; code=$?; run-p clean:localstorage; exit \"$code\"'",
    "test:dev": "sh -c 'vitest; code=$?; run-p clean:localstorage; exit \"$code\"'",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.9",
    "@mdi/font": "7.4.47",
    "date-fns": "^4.1.0",
    "firebase": "^12.9.0",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0",
    "vuefire": "^3.2.2",
    "vuetify": "^3.11.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@ionic/prettier-config": "^4.0.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/eslint-config-typescript": "^14.7.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-vuetify": "4.3.4",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass-embedded": "^1.97.3",
    "typescript": "~5.9.3",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1",
    "vite-plugin-vue-layouts-next": "^2.0.1",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,md,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "prettier": "@ionic/prettier-config"
}
